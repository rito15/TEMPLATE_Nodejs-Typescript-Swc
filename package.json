{
  "name": "------------------------------------------rename-this-------------------------------------",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "┌──[ ⭐ Run ]───────────────────────────────────────────────────────": "",
    "start:local:": "node -r @swc-node/register src/app.ts",
    "└───────────────────────────────────────────────────────────────": "",
    "┌──[ 👀 Watch ]──────────────────────────────────────────────────────": "",
    "watch:local" : "nodemon --watch src -e ts,tsx --watch .env.local --exec node -r @swc-node/register src/app.ts",
    "watch:dev"   : "set PROFILE=dev && nodemon --watch src -e ts,tsx --watch .env.dev --exec node -r @swc-node/register src/app.ts",
    "watch:prod"  : "set PROFILE=prod && nodemon --watch src -e ts,tsx --watch .env.prod --exec node -r @swc-node/register src/app.ts",
    "watch-error" : "tsc --noEmit --watch",
    "└──────────────────────────────────────────────────────────────-": "",
    "┌──[ 🔨 Build ]──────────────────────────────────────────────────────": "",
    "clean:dist": "rimraf dist",
    "build": "rimraf dist && npm run build:swc",
    "└────────────────────────────────────────────────────────────-──": "",
    "┌──[ 🔧 Install ]───────────────": "",
    "init": "npm install",
    "clean:node_modules": "rimraf node_modules",
    "install:deploy"    : "npm install --omit=dev --loglevel error",
    "re-install"        : "rimraf node_modules && npm install",
    "re-install:deploy" : "rimraf node_modules && npm run install:deploy",
    "└─────────────────────────────────────────────────────────────-─": "",
    "┌──[ 🌐 Serve ]──────────────────────────────────────────────────────": "",
    "serve:local": "node dist/app.js",
    "serve:dev"  : "set PROFILE=dev && node dist/app.js",
    "serve:prod" : "set PROFILE=prod && node dist/app.js",
    "re-serve:local": "npm run clean:dist && npm run build && npm run serve:local",
    "re-serve:dev"  : "npm run clean:dist && npm run build && npm run serve:dev",
    "re-serve:prod" : "npm run clean:dist && npm run build && npm run serve:prod",
    "└─────────────────────────────────────────────────────────-─────": "",
    "┌──[ 📚 Other ]──────────────────────────────────────────────────────": "",
    "build:swc": "swc ./src -d dist --copy-files --strip-leading-paths",
    "build:tsc": "tsc",
    "check:tsc": "tsc --noEmit",
    "└───────────────────────────────────────────────────────-───────": "",
    "┌──[ 🔒 AI ]────────────────────────────────────────────────────────": "",
    "run:gemini": "gemini",
    "run:claude": "claude",
    "install:gemini": "npm install -g @google/gemini-cli@latest",
    "install:claude": "npm install -g @anthropic-ai/claude-code@latest",
    "└──────────────────────────────────────────────────────-────────": "",
    "":""
  },
  "engines": {
    "node": ">=18",
    "typescript": ">=5"
  },
  "author": "Rito15",
  "keywords": [
  ],
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@types/node": "^14.14.37",
    "nodemon": "^2.0.22",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.2",
    "dotenv": "^8.2.0",
    "rimraf": "^6.0.1"
  }
}
